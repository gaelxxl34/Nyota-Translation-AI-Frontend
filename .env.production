# Production Environment Variables for NTC Frontend
# Copy this file to .env.production for production deployment

# Firebase Configuration (Production values)
VITE_FIREBASE_API_KEY=your_production_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-production-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-production-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-production-project-id.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_production_messaging_sender_id
VITE_FIREBASE_APP_ID=your_production_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_production_measurement_id

# Backend API Configuration (Production)
VITE_API_BASE_URL=https://your-backend-domain.com

# Frontend Configuration (Production)
VITE_FRONTEND_URL=https://your-frontend-domain.com
